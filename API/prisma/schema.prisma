generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = ("DATABASE_URL")
}

model LocalizacaoPA {
  id      Int    @id @default(autoincrement())
  filial  String
  Andar   String
  Espinha String
  PA      String
}

model RelacionamentoPA {
  id              Int      @id @default(autoincrement())
  LocalCompleto   String   @unique
  Patrimonio      String
  DataCricao      DateTime @default(now())
  DataModificacao DateTime @updatedAt
}
